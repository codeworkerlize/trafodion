{
    "traf-home": {
      "destination": "/opt/trafodion/esgynDB-_TRAF_VERSION_",
      "source": "traf_home",
      "priority": 10,
      "isDirectory": true
    },
    "traf-limits": {
      "destination": "/etc/security/limits.d/trafodion.conf",
      "source": "traf_home/sysinstall/etc/security/limits.d/trafodion.conf",
      "priority": 10,
      "isDirectory": false
    },
    "traf-sudoers": {
      "destination": "/etc/sudoers.d/trafodion",
      "source": "traf_home/sysinstall/etc/sudoers.d/trafodion",
      "priority": 10,
      "isDirectory": false
    },
    "traf-keepalive": {
      "destination": "/etc/keepalived/trafodion.conf",
      "source": "traf_home/conf/keepalived.conf",
      "priority": 10,
      "isDirectory": false
    }
}
