<!-- Put Trafodion site-specific property overrides in this file. -->

<configuration>
   <property>
    <name>fs.hdfs.impl.disable.cache</name>
    <value>false</value>
    <description>Setting it to false enables the caching of the HDFS FS meta object.</description>
   </property>
   <property>
    <name>hbase.coprocessor.region.classes</name>
      <value>
           org.apache.hadoop.hbase.coprocessor.transactional.TrxRegionObserver,
           org.apache.hadoop.hbase.coprocessor.transactional.TrxRegionEndpoint,
           org.apache.hadoop.hbase.coprocessor.AggregateImplementation
      </value>
   </property>
   <property>
    <name>hbase.client.scanner.timeout.period</name>
    <value>3600000</value>
   </property>
   <property>
    <name>hbase.client.keyvalue.maxsize</name>
    <value>0</value>
   </property>
   <property>
    <name>parquet.filter.record-level.enabled</name>
    <value>true</value>
   </property>
   <property>
    <name>parquet.filter.dictionary.enabled</name>
    <value>false</value>
   </property>
   <property>
    <name>parquet.filter.statistics.enabled</name>
    <value>true</value>
   </property>
   <property>
    <name>org.trafodion.sql.parquet.doublebuffer.enabled</name>
    <value>false</value>
   </property>
   <property>
    <name>org.trafodion.sql.parquet.fastserialize.enabled</name>
    <value>true</value>
   </property>
   <property>
    <name>org.trafodion.sql.parquet.concatblocks.enabled</name>
    <value>false</value>
   </property>
   <property>
    <name>org.trafodion.sql.parquet.concatblocks.size_mb</name>
    <value>8</value>
   </property>
   <property>
    <name>org.trafodion.sql.parquet.concatblocks.max</name>
    <value>16</value>
   </property>
   <property>
    <name>org.trafodion.sql.littlejetty.port</name>
    <value>8680</value>
   </property>
   <property>
    <name>org.trafodion.sql.littlejetty.cleanupintervalmn</name>
    <value>1440</value>
   </property>   
   <property>
    <name>org.trafodion.sql.parquet.timestamp.skip.conversion</name>
    <value>false</value>
    <description>whether to skip timezone conversion</description>
   </property>
   <property>
    <name>parquet.strings.signed-min-max.enabled</name>
    <value>true</value>
    <description>whether to enable usage of min-max for parquet strings</description>
   </property>
   <property>
    <name>org.apache.parquet.hadoop.parquetfilereader.use_direct_bytebuffer</name>
    <value>true</value>
    <description>whether to use direct bytebuffers when reading parquet files</description>
   </property>
   <property>
      <name>dcs.server.user.program.zookeeper.session.timeout</name>
      <value>3600</value>
   </property>
   <property>
      <name>zookeeper.session.timeout</name>
      <value>3600000</value>
  </property>
  <property>
      <name>backup.retention.period</name>
      <value>2</value>
      <description>The number of days to retain the backups before aging them out</description>
  </property>
  <property>
      <name>backup.retention.count</name>
      <value>48</value>
      <description>The number of recent backups to retain</description>
  </property>
</configuration>
