# Connectivity service start up script
SQLOG_DIR=$TRAF_LOG
SQMON_LOG=$SQLOG_DIR/sqmon.log

echo  "***"
echo  "*** Starting DCS"
echo  "***"
dcsstart
sleep 10

if [[ -z $CLUSTERNAME ]]; then
   reststart
fi

#comment out since it's unused right now
#echo  "***"
#echo  "*** Starting LOB Service"
#echo  "***"
#lobstart

if [[ -z $CLUSTERNAME ]]; then
   reststart
fi


sqcheck | tee -a $SQMON_LOG
