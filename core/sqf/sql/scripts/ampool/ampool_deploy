# Script to deploy esgyn coprocessor jar in the ampool env

. $TRAF_HOME/sql/scripts/ampool/setup_ampool_env

${AMPOOL_HOME}/bin/mash <<EOF

connect --locator=localhost[${AMPOOL_LOCATOR_PORT}]

list deployed
deploy --jar=${TRAF_HOME}/export/lib/${ESGYN_MONARCH_TRX_JAR}
list deployed
list functions

EOF

