  update test_tb set c3 = c3*10 ,c4 = c4*10 where c1 in (2,3);
commit;
select * from test_tb;
begin work;
  update test_tb set c3 = c3*10 ,c4 = c4*10 where c1 in (2,3);
rollback;
select * from test_tb;

