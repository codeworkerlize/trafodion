      insert into tb_timestamp_log values
        (systimestamp,'delete','c_timestamp',old.c_timestamp,null),
        (systimestamp,'delete','c_timestamp_0',old.c_timestamp_0,null),
        (systimestamp,'delete','c_timestamp_1',old.c_timestamp_1,null),
