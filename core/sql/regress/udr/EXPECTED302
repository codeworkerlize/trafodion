>>CREATE OR REPLACE PROCEDURE test_with_stmt() AS//
BEGIN
 WITH tmp AS (select
    to_char(TIMESTAMP '2019-06-24 00:00:00.000000', 'yyyy') as yyyy
    from dual)
select
   CASE 'yyyy' 
   WHEN 'yyyy' THEN yyyy
   ELSE 'date format ' || coalesce('yyyymmdd', '  ') || ' not recognized.' END
 from tmp;
END//;

