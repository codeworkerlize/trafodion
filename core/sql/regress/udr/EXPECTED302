>>create or replace package pkg as //
  i  INT := 6;
  j  NUMERIC(10,2) ;
  k  INT;
  t2row t2%ROWTYPE;
  t2a  t2.a%TYPE;
  t2cur CURSOR for select * from t2;
end//;

--- SQL operation complete.
>>
>>create or replace procedure test_pack_vars() as //
begin
  open pkg.t2cur;
  fetch pkg.t2cur into pkg.t2row;
  print pkg.t2row.a;
  close pkg.t2cur;
end//;

--- SQL operation complete.
