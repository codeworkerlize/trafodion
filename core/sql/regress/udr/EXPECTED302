>>create or replace procedure print_sqlcode() as //
begin
  declare cnt int;
  declare continue handler for sqlexception
  begin
    print sqlcode;
    print sqlcode;
  end;

  select count(*) into cnt from dual;
  print activity_count;
  print cnt;
  print activity_count;

  select * from table_not_exist;
  print sqlcode;
  print sqlcode;
end//;

