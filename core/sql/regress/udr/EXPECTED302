>>CREATE OR REPLACE PROCEDURE t_p() AS//
BEGIN
   DECLARE id INT;
   DECLARE t_row t5%ROWTYPE;;
   DECLARE CURSOR cur FOR SELECT * FROM t5;
   PRINT('open cursor now!');
   OPEN cur;
   FETCH cur INTO t_row;
   LOOP
     SET id = t_row.a;
     print id;
     FETCH cur INTO t_row;
     EXIT WHEN cur%NOTFOUND;
   END LOOP;
   CLOSE cur;
END//;

--- SQL operation complete.
