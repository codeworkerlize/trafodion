>>create or replace procedure test_decimal() as //
declare
  d decimal(5,3);
  e decimal(5,2);
  f decimal(3,1);
begin
  select decimal_func() from dual;
  select decimal_func() into d from dual;
  select decimal_func() into e from dual;
  select decimal_func() into f from dual;
  print d;
  print e;
  print f;
end
//;

