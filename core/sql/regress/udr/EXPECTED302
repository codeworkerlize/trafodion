>>create or replace procedure test_cursor_with_params() as//
begin
  declare v1 int;
  declare v2 varchar(10);
  declare c(i int,j varchar) cursor is select * from t302_tab_test_cursor_with_params where a = i and b = j;
  open c(1, 'a');
  fetch c into v1,v2;
  print v1;
  print v2;
  close c;
  /* close cursor and open */
  open c(2, 'b');
  fetch c into v1,v2;
  print v1;
  print v2;
  close c;
end//;

