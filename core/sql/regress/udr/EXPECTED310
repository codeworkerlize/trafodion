>>-- Mantis 16212 存储过程执行整数数学运算,会出现小数点
>>--
>>
>>create or replace PROCEDURE m16212p1() as //
declare
  v_a int;
  v_b varchar(20);
begin
  v_a:=20;
  select to_number(nvl(cast(v_a as varchar(20)),'0'))-1 into v_b from dual;
  print v_b;
end//;

--- SQL operation complete.
>>
>>call m16212p1();

