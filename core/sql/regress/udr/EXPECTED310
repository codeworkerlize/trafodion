>>create or replace PROCEDURE m16741p1() as //
declare
  v1 varchar(20);
  v2 int;
begin
   v1 = case 3
           when 0 then 'failed'
           when 1 then 'failed'
           else 'correct'
         end ;
   print v1;

   v2 = case 'a'
           When 'b' then 1
           when 'c' then 2
           else 3
         end ;
   print v2;
end//;

