begin
  if inserting then
      update tb_prepared_log set oper_desc = 'insert', oper_query = ACTION_SQL ;
  elsif updating then
      update tb_prepared_log set oper_desc = 'update', oper_query = ACTION_SQL ;
  elsif deleting then
      update tb_prepared_log set oper_desc = 'delete', oper_query = ACTION_SQL ;
  else
      null;
  end if;
end;;

