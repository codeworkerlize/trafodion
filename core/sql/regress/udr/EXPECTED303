>>create or replace trigger test_tb_trig
+>after insert or update or delete
+>on test_tb
+>for each row
+>as//
begin
 if inserting then
  insert into test_tb_log values(:new.c1);
 elsif updating then
  insert into test_tb_log values(:old.c1);
  insert into test_tb_log values(:new.c1);
 elsif deleting then
  insert into test_tb_log values(:old.c1);
 else
  null;
 end if;
end;//;

