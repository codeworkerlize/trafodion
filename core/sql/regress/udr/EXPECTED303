   :new.c_dec_ps_s_m ,
   :new.c_dec_ps_us_m ,
   :new.c_dec_p_m ,
   :new.c_dec_p_s_m ,
   :new.c_dec_p_us_m
   );
 elsif deleting then
   insert into tb_decimal_1
   values
   (
   :old.id ,
   :old.insert_timestamp ,
   :old.c_dec_ps_min ,
   :old.c_dec_ps_s_min ,
   :old.c_dec_ps_us_min ,
   :old.c_dec_ps_max ,
