fil=$1
if [ "$fil" = "" ]; then
  echo "Usage: $0 filename"
  exit 1
fi

# cluster size defaults to number of nodes in most cases, 
# until line 749 tenant4 cluster size is specified as 1
sed "
1,750s/CLUSTER SIZE [1-9]/CLUSTER SIZE N/
s/] With the new tenant definition for tenant .*/] %oversubscription message%/
s/^  -- Tenant node assignments:.*//g
" $fil
