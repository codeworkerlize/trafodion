fil=$1
sed "
s/_[0-9][0-9]*/@TAG_TS@/g
s/java:[0-9]*/@LINE##@/
s/StartTime: *[0-9 :\-]*/StartTime: <time removed>/g
s/EndTime: *[0-9 :\-]*/EndTime: <time removed>/g
s/[0-9][0-9]:[0-9][0-9]:[0-9][0-9]/hh:mm:ss/g
#s/ET: *[0-9:]*/EndTime: <time removed>/g
s/'hdfs:\/\/.*.schgsexport'/@HDFS@/
s/LOBCHUNKS__[0-9]*/@LOBCHUNKS@/g
" $fil
