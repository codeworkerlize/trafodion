fil=$1
sed "
s/^[A-Za-z0-9. :_]*T1_[0-9]\{1,10\}_[0-9]\{1,10\}[A-Za-z0-9. ]*/@T1UNIQUEKEY@/g
s/^[A-Za-z0-9. :_]*T2_[0-9]\{1,10\}_[0-9]\{1,10\}[A-Za-z0-9. ]*/@T2UNIQUEKEY@/g
#s/^[A-Za-z0-9. :_]*[0-9]\{4\}-[0-9]\{2\}-[0-9]\{2\} [0-9]\{2\}:[0-9]\{2\}:[0-9]\{2\}.[0-9]\{4,6\}[A-Za-z0-9. ]*/@TIMESTAMP@/g
#s/^[A-Za-z0-9. :_()]*[0-9]\{4\}-[0-9]\{2\}-[0-9]\{2\} [0-9]\{2\}:[0-9]\{2\}:[0-9]\{2\}/@TIMESTAMP@/g
#s/^[A-Za-z0-9. :_()]*[0-9]\{4\}-[0-9]\{2\}-[0-9]\{2\}:[0-9]\{2\}:[0-9]\{2\}:[0-9]\{2\}/@TIMESTAMP@/g
s/[0-9]\{4\}-[0-9]\{2\}-[0-9]\{2\}:[0-9]\{2\}:[0-9]\{2\}:[0-9]\{2\}/@TIMESTAMP@/g
s/Table ID: *[0-9]*/Table ID: <ID removed>/g
s:^[ ]\{0,9\}[0-9]\{1,10\}:0000000000:
s/LOBH[0-9]*/@LOBHANDLE@/
s/LOBP_[0-9]*/@LOBP@/g
s/LOBDescChunks__[0-9]*/@LOBDescChunks@/
s/LOBDescChunks__[0-9]_[0-9]_[0-9]*/@LOBDescChunks_PK@/
s/LOBDescHandle__[0-9]*/@LOBDescHandle@/
s/LOBMD__[0-9]*/@LOBMD@/
s/LOBMD__[0-9]_[0-9]_[0-9]*/@LOBMD_PK@/
s/LOBCHUNKS__[0-9]*/@LOBCHUNKS@/g
s/'hdfs:\/\/.*.backups'/@HDFS@/
s/Time:.*/Time:@time@/
s/java:[0-9]*/@LINE##@/
" $fil
