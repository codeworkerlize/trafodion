--DD.MM.YYYY HH24.MI.SS.FF
select to_char(TIMESTAMP '2016-03-01 12:05:10.196000','DD.MM.YYYY HH24.MI.SS.FF') from dual;
select to_date ('03.01.2016 14.05.10.196000000','DD.MM.YYYY HH24.MI.SS.FF') from dual;
select to_char(TIMESTAMP '2020-04-02 02:00:00.01', 'DD.MM.YYYY HH24.MI.SS.FF') from dual;

--YYYY/MM/DD HH24:MI:SS.FF
