select to_date ('2016-01-03:14:05:10.196000000','YYYY-MM-DD:HH24:MI:SS.FF') from dual;
select to_char(TIMESTAMP '2020-04-02 02:00:00.000000001','YYYY-MM-DD:HH24:MI:SS.FF') from dual;

--YYYYMMDD HH24:MI:SS.FF
select to_char(TIMESTAMP '2020-04-02 02:00:00.000000001','YYYYMMDD HH24:MI:SS.FF') from dual;
select to_date ('20200402 02:00:00.000000001','YYYYMMDD HH24:MI:SS.FF') from dual;

--TO_NUMBER
select to_number(12345) from dual;
select to_number(123.2258) from dual;
select to_number('123') from dual;
select to_number('123.456') from dual;
prepare s from select to_number(?) from dual;
execute s using '123';
execute s using '123.456';

--TO_CHAR NUMBERFORMAT
drop table if exists t030t2;
create table t030t2(c1 numeric(38,8));
insert into t030t2 values(1.2),(1.23456789),(1.2345678),(3),(123456798.9),(0),(-1.2),(-1.23456789),(-1.2345678),(-3),(-123456798.9);
select c1,to_char(c1,'999999990.9999990') from t030t2;
select c1,to_char(c1,'999999990.99999990') from t030t2;
select c1,to_char(c1,'999999990.999999990') from t030t2;
drop table if exists t030t3;
create table t030t3(c1 numeric(18,6));
insert into t030t3 values(1.2),(1.234567),(3),(123456798.9),(0),(-1.2),(-1.234567),(-3),(-123456798.9);
select c1,to_char(c1,'999999990.99990') from t030t3;
